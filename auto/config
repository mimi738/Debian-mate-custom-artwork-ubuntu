#!/bin/sh

lb config noauto \
	--clean \
	--architectures i386 \
	--mode debian \
	--distribution  bullseye \
	--initramfs live-boot \
	--archive-areas "main contrib non-free" \
	--debian-installer live \
	--linux-flavours "686 686-pae" \
	--linux-packages "linux-image" \
	--source "false" \
"${@}"
